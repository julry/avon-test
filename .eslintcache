`\n\nconst Image = styled.img`\n    transform: scale(-1, 1);\n    height: 115%;\n    @media screen and (min-width: 640px){\n        transform: none;\n        height: 100%;\n    }\n    \n    \n`\n\nexport const QuestionWrapper = props => {\n    const {question} = props;\n    const [questionAnswers, setQuestionAnswers] = useState(question.answers);\n    const [isNextAvailable, setIsNextAvailable] = useState(false);\n    const {answers, setAnswer, setNext} = useContext(ProgressContext);\n    const isLastQuestion = +question.id===questions.length;\n\n\n    const onNext = () => {\n        setNext();\n    };\n\n    const handleAnswerChange = useCallback((answerId) => {\n        setAnswer(question.id, answerId);\n        setIsNextAvailable(true);\n    }, [question, setAnswer]);\n\n    return (\n        <Wrapper>\n            <Question> {question.text} </Question>\n            <AnswersBoxStyled last={isLastQuestion}>\n                {questionAnswers.map(answer => (\n                    <RadioButtonStyled\n                        key={answer.id}\n                        name={question.id}\n                        value={answer.id}\n                        checked={answers[question.id]===answer.id}\n                        onChange={handleAnswerChange}\n                    >\n                        {answer.text}\n                    </RadioButtonStyled>\n                ))}\n            </AnswersBoxStyled>\n            {isNextAvailable && <NextButton text={'Далее'} onClick={onNext}/>}\n\n            <ImageWrapper>\n                <Image src={questionImage} alt={''}/>\n            </ImageWrapper>\n        </Wrapper>\n    );\n};\n",["150","151"],"/Users/juliaryseva/Desktop/projects/avon-test/src/components/screens/questions/Question2.jsx",[],"/Users/juliaryseva/Desktop/projects/avon-test/src/questions.config.js",[],"/Users/juliaryseva/Desktop/projects/avon-test/src/components/svgIcons/Skills/HumanityLoveIcon.jsx",[],"/Users/juliaryseva/Desktop/projects/avon-test/src/componen